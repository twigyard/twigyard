        "dflydev/fig-cookies": "^1.0",
        "nette/caching": "^2.5.6",
        "monolog/monolog": "^1.23",
        "symfony/config": "^4.0.4",
        "symfony/validator": "^4.0.4",
        "symfony/console": "^4.0.4",
        "symfony/finder": "^4.0.4",
        "symfony/dependency-injection": "^4.0.4",
