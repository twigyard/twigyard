        "twig/extensions": "^1.3",
        "zendframework/zend-diactoros": "^1.3.7",
        "swiftmailer/swiftmailer": "^5.4",
        "html2text/html2text": "^4.0.1",
        "dflydev/fig-cookies": "^1.0",
        "nette/caching": "^2.4",
        "monolog/monolog": "^1.19",
        "symfony/config": "^3.1",
        "symfony/validator": "^3.1",
        "symfony/console": "^3.1",
        "symfony/finder": "^3.1"
    },
    "require-dev": {
        "codeception/codeception": ">=2.2.11",
